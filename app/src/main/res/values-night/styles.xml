<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="groupColor">

        <item name="android:textColor">@color/ColorNote5</item>
        <item name="android:textSize">@dimen/_11ssp</item>
        <item name="android:fontFamily">@font/ubuntu_medium</item>

    </style>

    <style name="Toolbar" parent="ThemeOverlay.AppCompat.Light">

        <item name="android:textColor">@color/ColorNote5</item>
        <item name="android:fontFamily">@font/ubuntu_bold</item>
        <item name="android:textSize">@dimen/_20ssp</item>

    </style>


</resources>